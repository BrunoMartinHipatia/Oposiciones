<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" >

    <application
        android:name=".OposicionesApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Oposiciones"
        tools:targetApi="31" >
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExamenesContainerActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones" >

        </activity>
        <activity
            android:name=".ExamenesActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones" >

        </activity>
        <activity
            android:name=".PreguntasActivity"
            android:exported="true"
            android:screenOrientation="landscape"
            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones"
            tools:ignore="DiscouragedApi">

        </activity>
        <activity
            android:name=".ExamenesGuardadosActivity"
            android:exported="true"

            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones"
            tools:ignore="DiscouragedApi">

        </activity>
        <activity
            android:name=".FlashCardsActivity"
            android:exported="true"

            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones"
            tools:ignore="DiscouragedApi">

        </activity>
        <activity
            android:name=".PreguntasGuardadasActivity"
            android:exported="true"
            android:screenOrientation="landscape"
            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones"
            tools:ignore="DiscouragedApi">

        </activity>
        <activity
            android:name=".ExaminateActivity"
            android:exported="true"
            android:screenOrientation="landscape"
            android:label="@string/app_name"
            android:theme="@style/Theme.Oposiciones"
            tools:ignore="DiscouragedApi">

        </activity>
    </application>

</manifest>